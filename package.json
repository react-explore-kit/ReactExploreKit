{
  "name": "react-explore-kit",
  "version": "1.0.1",
  "description": "",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md,mdx}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build && changeset publish"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VedanthB/ReactExploreKit.git"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "8.5.0",
    "launchit": "1.1.2",
    "turbo": "^1.10.16",
    "typescript": "^5.2.2"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VedanthB/ReactExploreKit/issues"
  },
  "homepage": "https://github.com/VedanthB/ReactExploreKit#readme",
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
